{
    "title": "WHY' Learning Notebook",
    "author": "Wang",
    "description": "Study With Me",
    "language": "zh-hans",
    "structure": {
        "readme": "README.md"
    },
    "links": {
        "sidebar": {
            "Lagacy": "https://lagacy.whynbnb.com"
        }
    },
    "theme-default": {
        "showLevel": true
    },
    "plugins": [
        "-sharing",
        "splitter",
        "expandable-chapters-small",
        "anchors",
        "github",
        "github-buttons",
        "sharing-plus",
        "mathjax"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/whybbe"
        },
        "github-buttons": {
            "buttons": [{
                "user": "whybbe",
                "repo": "Linux",
                "type": "star",
                "size": "big",
                "count": true
                }
            ]
        },
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "qq": true,
            "qzone": false,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": true,
            "whatsapp": false,
            "all": [
                "google", "facebook", "weibo", "twitter",
                "qq", "qzone", "linkedin", "pocket"
            ]
        }
    }
}